{
  "ReverseProxy": {
    "Routes": {
      "catalog": {
        "ClusterId": "catalog-cluster",
        "Match": { "Path": "/catalog/{**catchall}" },
        "Transforms": [ { "PathRemovePrefix": "/catalog" } ]
      },
      "order": {
        "ClusterId": "order-cluster",
        "Match": { "Path": "/order/{**catchall}" },
        "Transforms": [ { "PathRemovePrefix": "/order" } ]
      }
    },
    "Clusters": {
      "catalog-cluster": {
        "Destinations": { "d1": { "Address": "http://localhost:5081/" } }
      },
      "order-cluster": {
        "Destinations": { "d1": { "Address": "http://localhost:5082/" } }
      }
    }
  }
}
