{
  "ReverseProxy": {
    "Routes": {
      "autonego": {
        "ClusterId": "autonego",
        "Match": { "Path": "/autonego/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/autonego" } ]
      },
      "checkquota": {
        "ClusterId": "checkquota",
        "Match": { "Path": "/checkquota/{**catch-all}" },
        "Transforms": [ { "PathRemovePrefix": "/checkquota" } ]
      }
    },
    "Clusters": {
      "autonego": {
        "Destinations": {
          "d1": { "Address": "http://autonego:8080/" }
        }
      },
      "checkquota": {
        "Destinations": {
          "d1": { "Address": "http://checkquota:8081/" }
        }
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": { "Url": "http://0.0.0.0:8000" }
    }
  }
}
