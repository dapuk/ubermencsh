{
  "ReverseProxy": {
    "Routes": {
      "autonego": {
        "ClusterId": "autonego",
        "Match": { "Path": "/AutoNego/{**catch-all}" }
      },
      "checkquota": {
        "ClusterId": "checkquota",
        "Match": { "Path": "/CheckQuota/{**catch-all}" }
      }
    },
    "Clusters": {
      "autonego": {
        "Destinations": {
          "d1": { "Address": "http://*:8080/" }
        }
      },
      "checkquota": {
        "Destinations": {
          "d1": { "Address": "http://*:8081/" }
        }
      }
    },
    "Kestrel": {
      "Endpoints": {
        "Http": { "Url": "http://*:8000" }
      }
    }
  }
}